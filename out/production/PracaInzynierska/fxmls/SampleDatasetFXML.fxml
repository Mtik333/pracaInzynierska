<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" xmlns="http://javafx.com/javafx/8"
            fx:controller="controllers.SampleDatasetFXMLController">
    <VBox>
        <HBox layoutX="15.0" layoutY="15.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Pane HBox.hgrow="ALWAYS"/>
            <Label prefHeight="40.0" text="Select sample dataset" textAlignment="CENTER"/>
            <Pane HBox.hgrow="ALWAYS"/>
        </HBox>
        <HBox>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Pane HBox.hgrow="ALWAYS"/>
            <ComboBox fx:id="exampleDataset" prefHeight="40.0"/>
            <Pane layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS"/>
        </HBox>
        <HBox layoutX="15.0" layoutY="115.0">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Pane HBox.hgrow="ALWAYS"/>
            <Label prefHeight="40.0" text="Description"/>
            <Pane HBox.hgrow="ALWAYS"/>
        </HBox>
        <HBox layoutX="15.0" layoutY="115.0">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <TextArea fx:id="datasetDescription" prefWidth="400.0"/>
        </HBox>
        <HBox>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Pane layoutX="90.0" layoutY="10.0" HBox.hgrow="ALWAYS"/>
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#loadFile"
                    prefHeight="25.0" prefWidth="70.0" text="OK">
                <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </HBox.margin>
            </Button>
            <Pane HBox.hgrow="ALWAYS"/>
        </HBox>
    </VBox>
</AnchorPane>
