<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxWidth="600.0" prefHeight="103.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SetSeparatorFXMLController">
   <children>
      <VBox prefHeight="103.0" prefWidth="300.0">
         <children>
            <HBox prefHeight="31.0" prefWidth="280.0" VBox.vgrow="NEVER">
               <children>
                  <Label prefHeight="31.0" prefWidth="65.0" text="Separator:" HBox.hgrow="NEVER">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <ChoiceBox fx:id="separators" prefHeight="31.0" prefWidth="216.0" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></ChoiceBox>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="65.0" prefWidth="300.0">
               <children>
                  <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancelSet" prefHeight="25.0" prefWidth="250.0" text="Cancel">
                     <HBox.margin>
                        <Insets left="10.0" right="50.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#setSeparator" prefHeight="25.0" prefWidth="250.0" text="Set">
                     <HBox.margin>
                        <Insets left="50.0" right="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
