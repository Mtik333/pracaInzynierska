<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ShowVerticeXMLController">
    <VBox>
        <HBox>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Label prefHeight="40.0" prefWidth="180.0" text="Name:"/>
            <TextField fx:id="verticeName" prefHeight="40.0" prefWidth="220.0"/>
        </HBox>
        <HBox layoutX="10.0" layoutY="50.0">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Label prefHeight="40.0" prefWidth="180.0" text="Amount of ants (single step):"/>
            <TextField fx:id="antsAmount" editable="false" prefHeight="40.0" prefWidth="220.0"/>
        </HBox>
        <HBox layoutX="15.0" layoutY="115.0">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Label prefHeight="40.0" prefWidth="180.0" text="Part of current reduct?"/>
            <TextField fx:id="isInReduct" editable="false" prefHeight="40.0" prefWidth="220.0"/>
        </HBox>
        <HBox>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </VBox.margin>
            <Pane layoutX="90.0" layoutY="10.0" HBox.hgrow="ALWAYS"/>
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#dismiss"
                    prefHeight="25.0" prefWidth="70.0" text="OK">
                <HBox.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </HBox.margin>
            </Button>
            <Pane HBox.hgrow="ALWAYS"/>
        </HBox>
    </VBox>
</AnchorPane>
