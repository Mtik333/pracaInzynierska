<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SampleDatasetFXMLController">
   <children>
       <VBox>
          <children>
              <HBox layoutX="15.0" layoutY="15.0" prefWidth="300.0" VBox.vgrow="ALWAYS">
                  <VBox.margin>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
                 <children>
                     <Pane HBox.hgrow="ALWAYS" />
                     <Label prefHeight="40.0" text="Select sample dataset" textAlignment="CENTER" />
                     <Pane HBox.hgrow="ALWAYS" />
                 </children>
              </HBox>
              <HBox>
                  <VBox.margin>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
                 <children>
                     <Pane HBox.hgrow="ALWAYS" />
                     <ComboBox fx:id="exampleDataset" prefHeight="40.0" />
                     <Pane layoutX="10.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                 </children>
              </HBox>
              <HBox layoutX="15.0" layoutY="115.0">
                  <VBox.margin>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
                 <children>
                     <Pane HBox.hgrow="ALWAYS" />
                     <Pane HBox.hgrow="ALWAYS" />
                 </children>
              </HBox>
              <HBox layoutX="15.0" layoutY="115.0">
                  <VBox.margin>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
              </HBox>
              <HBox>
                  <VBox.margin>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
                 <children>
                     <Pane layoutX="90.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
                     <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#loadFile" prefHeight="25.0" prefWidth="70.0" text="OK">
                         <HBox.margin>
                             <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                         </HBox.margin>
                     </Button>
                     <Pane HBox.hgrow="ALWAYS" />
                 </children>
              </HBox>
          </children>
       </VBox>
   </children>
</AnchorPane>
