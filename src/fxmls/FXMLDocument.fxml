<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FXMLDocumentController">
    <children>
        <MenuBar prefHeight="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#loadDataset" text="Load dataset from file" />
                  <MenuItem mnemonicParsing="false" onAction="#loadSampleDataset" text="Load sample dataset" />
                        <MenuItem mnemonicParsing="false" onAction="#setSeparator" text="Set separator" />
                        <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Options">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#programSettings" text="Algorithm settings" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <HBox alignment="TOP_CENTER" layoutY="25.0" maxHeight="1.7976931348623157E308">
            <children>
                <Button fx:id="viewExamples" disable="true" mnemonicParsing="false" onAction="#editExamples" prefHeight="66.0" prefWidth="150.0" text="View examples in a table view" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
                <Button fx:id="manualAnts" disable="true" layoutX="330.0" layoutY="10.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="150.0" text="Initialize ants (manually)" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
                <Button fx:id="nextEdge" disable="true" layoutX="170.0" layoutY="10.0" mnemonicParsing="false" onAction="#antsOneStep" prefHeight="66.0" prefWidth="150.0" text="Step to next edge selection in single iteration" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
                <Button fx:id="singleIteration" disable="true" layoutX="490.0" layoutY="10.0" mnemonicParsing="false" onAction="#antsOneIteration" prefHeight="66.0" prefWidth="150.0" text="Compute single iteration" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
                <Button fx:id="singleReduct" disable="true" mnemonicParsing="false" onAction="#antsFindReduct" prefHeight="66.0" prefWidth="150.0" text="Compute reduct" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
                <Button fx:id="showStatistics" disable="true" mnemonicParsing="false" prefHeight="66.0" prefWidth="150.0" text="Show statistics" HBox.hgrow="ALWAYS" />
            <Button fx:id="resetAlgorithm" disable="true" layoutX="760.0" layoutY="10.0" mnemonicParsing="false" onAction="#resetAlgorithm" prefHeight="66.0" prefWidth="150.0" text="Reset algorithm" />
            </children>
        </HBox>
        <TabPane fx:id="tabPane" layoutX="-30.0" layoutY="86.0" prefHeight="514.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="86.0">
            <tabs>
                <Tab text="Solve">
                    <content>
                        <AnchorPane fx:id="solver" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
