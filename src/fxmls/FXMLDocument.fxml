<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FXMLDocumentController">
    <children>
      <MenuBar prefHeight="25.0" prefWidth="1028.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#loadDataset" text="Load dataset" />
                  <MenuItem mnemonicParsing="false" onAction="#setSeparator" text="Set separator" />
              <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Options">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#programSettings" text="Algorithm settings" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox layoutY="25.0" prefHeight="66.0" prefWidth="1028.0">
         <children>
            <Button mnemonicParsing="false" onAction="#editExamples" prefHeight="66.0" prefWidth="139.0" text="View examples in a table view" textAlignment="CENTER" wrapText="true" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#antsRandomButton" prefHeight="66.0" prefWidth="128.0" text="Initialize ants (random)" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" />
            <Button layoutX="330.0" layoutY="10.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="160.0" text="Initialize ants (manually)" textAlignment="CENTER" wrapText="true" />
            <Button layoutX="170.0" layoutY="10.0" mnemonicParsing="false" onAction="#antsOneStep" prefHeight="66.0" prefWidth="160.0" text="Step to next edge selection in single iteration" textAlignment="CENTER" wrapText="true" />
            <Button layoutX="490.0" layoutY="10.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="160.0" text="Compute single iteration" textAlignment="CENTER" wrapText="true" />
            <Button mnemonicParsing="false" prefHeight="66.0" prefWidth="131.0" text="Compute reduct" textAlignment="CENTER" wrapText="true" />
            <Button mnemonicParsing="false" prefHeight="66.0" prefWidth="156.0" text="Show statistics" />
         </children>
      </HBox>
      <TabPane fx:id="tabPane" layoutX="-30.0" layoutY="86.0" prefHeight="514.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="86.0">
        <tabs>
          <Tab text="Create">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="800.0">
                     <children>
                        <TextArea fx:id="examplesToString" editable="false" layoutX="26.0" layoutY="22.0" prefHeight="450.0" prefWidth="970.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Solve">
            <content>
              <AnchorPane fx:id="solver" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
    </children>
</AnchorPane>
