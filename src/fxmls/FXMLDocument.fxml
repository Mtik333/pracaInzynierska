<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefWidth="912.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FXMLDocumentController">
   <children>
       <MenuBar prefHeight="25.0" prefWidth="912.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
          <menus>
              <Menu mnemonicParsing="false" text="File">
                 <items>
                     <MenuItem mnemonicParsing="false" onAction="#openDatasetDialog" text="Load dataset from file" />
                     <MenuItem mnemonicParsing="false" onAction="#loadSampleDataset" text="Load sample dataset" />
                     <MenuItem mnemonicParsing="false" onAction="#setSeparator" text="Set separator" />
                     <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
                 </items>
              </Menu>
              <Menu mnemonicParsing="false" text="Options">
                 <items>
                     <MenuItem mnemonicParsing="false" onAction="#antProgramSettings" text="Ant algorithm settings" />
                  <MenuItem mnemonicParsing="false" onAction="#fishProgramSettings" text="Fish algorithm settings" />
                 </items>
              </Menu>
          </menus>
       </MenuBar>
       <HBox alignment="TOP_CENTER" layoutY="25.0" maxHeight="1.7976931348623157E308" prefHeight="66.0" prefWidth="12.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
          <children>
              <Button fx:id="viewExamples" disable="true" mnemonicParsing="false" onAction="#editExamples" prefHeight="66.0" prefWidth="230.0" text="View dataset in a table view" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
              <Button fx:id="singleReduct" disable="true" mnemonicParsing="false" onAction="#antsFindReduct" prefHeight="66.0" prefWidth="228.0" text="Compute reduct (ants)" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
              <Button fx:id="fishSingleReduct" disable="true" mnemonicParsing="false" onAction="#calculateFish" prefHeight="66.0" prefWidth="228.0" text="Compute reduct (fish)" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
              <Button fx:id="resetAlgorithm" disable="true" mnemonicParsing="false" onAction="#resetAlgorithm" prefHeight="66.0" prefWidth="228.0" text="Reset algorithm" wrapText="true" HBox.hgrow="ALWAYS" />
          </children>
       </HBox>
       <TabPane fx:id="tabPane" layoutX="-30.0" layoutY="91.0" prefHeight="579.0" prefWidth="912.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="91.0">
          <tabs>
              <Tab text="Solve">
                 <content>
                     <AnchorPane fx:id="solver" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                 </content>
              </Tab>
          </tabs>
       </TabPane>
   </children>
</AnchorPane>
